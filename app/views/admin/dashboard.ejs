<script type="text/javascript">
    alert('ce qui suit vient de nodejs');
var dataPreferences = {
	series: [
		[25, 30, 20, 25]
	]
};

var optionsPreferences = {
	donut: true,
	donutWidth: 40,
	startAngle: 0,
	total: 100,
	showLabel: false,
	axisX: {
		showGrid: false
	}
};


// Vote 1 statistics 
Chartist.Pie('#chartVote1', dataPreferences, optionsPreferences);

Chartist.Pie('#chartVote1', {
  labels: ['33%','77%'],
  series: [33, 77 ]
});

